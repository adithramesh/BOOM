        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Products List</h2>
                    <p>List of all products.</p>
                </div>
                <div>
                    <a href="/admin/add-products" class="btn btn-primary btn-sm rounded">Create new</a>
                </div>
            </div>
           
                </header> <!-- card-header end// -->
                <% if (messages.success) { %>
                    <div class="alert alert-success" role="alert">
                        <%= messages.success %>
                    </div>
                <% } %>
                    
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>Sl. No.</th>
                            <th>Name</th>
                            <th>Description</th>
                            <th>Price</th>
                            <th>Status</th>
                            <th>Images</th>
                            <th>Quantity</th>
                            <th>Colour</th>
                            <th>Category</th>
                            <!-- <th>Subcategory</th> -->
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% products.forEach((product, index) => { %>
                            <tr>
                                <td><%= index + 1 %></td>
                                <td><%= product.name %></td>
                                <td><%= product.description %></td>
                                <td>$<%= product.price.toFixed(2) %></td>
                                <!-- <td><%= product.isBlocked ? 'Blocked' : 'Active' %></td> -->
                                <td>
                                    <% if(product.isBlocked) { %>
                                        <span class="badge bg-danger">Blocked</span>
                                    <% } else { %>
                                        <span class="badge bg-success">Active</span>
                                    <% } %>
                                </td>
                                <td>
                                    <img src="<%= product.images[0] %>" width="65"
                                        class="img-thumbnail" >
                                </td>


                                <!-- <td>
                                    <ul class="list-unstyled">
                                        <% product.images.forEach(image => { %>
                                            <li><img src="/uploads/<%= image %>" width="55"  class="img-thumbnail"></li>
                                        <% }); %>
                                    </ul> -->
                                </td>
                                <td><%= product.quantity %></td>
                                <td><%= product.colour %></td>
                                <td><%= product.category ? product.category.name : 'No category' %></td>
                                
                                <td>
                                    <form action="/admin/update-product-status" method="POST" style="display: inline;">
                                        <input type="hidden" name="productId" value="<%= product._id %>">
                                        <button type="submit" class="btn btn-sm btn-success">Change Status</button>
                                    </form>
                                    <form action="/admin/edit-product/<%= product._id %>" method="GET" style="display: inline;">
                                        <button type="submit" class="btn btn-sm btn-primary">Edit</button>
                                    </form>
                                </td>
                            </tr>
                        <% }); %>
                    </tbody>
                </table>
                        
              
